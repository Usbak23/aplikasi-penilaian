<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h3 class="m-0 font-weight-bold text-primary">Data Nilai Makalah Peserta</h3>
    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <div class="table-responsive">

            <table id="example1" class="table table-bordered" >
               <thead style="text-align: center;">
        <tr>
          <th rowspan="2">No.</th>
          <th rowspan="2">Nama</th>
              <th colspan="3">Makalah</th>
          <th rowspan="2">Total</th>
        </tr>
        <tr>
              <th>Makalah</th>
              <th>Penyampaian</th>
              <th>Respons</th>
        </tr>
      </thead>
      <tbody>
        <% if (makalahPeserta.length > 0) { %>
          <% makalahPeserta.forEach((data index) => { %>
            <tr>
              <td><%= index + 1 %></td>
              <td><%= data.pesertaName %></td>
              <td><%= data.pesertaCabang %></td>
                <td><%= dataMakalah %></td>
                <td><%= dataPenyampaian %></td>
                <td><%= dataRespons %></td>
              <% }) %>
              <td><%= data.Jumlah %></td>
            </tr>
          <% }) %>
        <% } else { %>
          <tr>
            <td colspan="<%= 2 + materi.length * 3 %>" style="text-align: center;">Data tidak tersedia</td>
          </tr>
        <% } %>
      </tbody>
        </table>
    </div>
    </div>
    <!-- /.card-body -->
</div>