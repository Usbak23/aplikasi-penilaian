<div class="card shadow mb-4">
  <div class="card-header py-3">
    <h3 class="m-0 font-weight-bold text-primary">Data Nilai Makalah Peserta</h3>
  </div>
  <!-- /.card-header -->
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-bordered table-hover">
        <thead class="text-center">
          <tr>
            <th>No</th>
            <th>Nama Peserta</th>
            <th>Asal Cabang</th>
            <th>Nilai Penulisan</th>
            <th>Nilai Penyampaian</th>
            <th>Nilai Respon</th>
            <th>Aksi</th>
            <th>Jumlah</th>
          </tr>
        </thead>
        <tbody>
          <% for(let i=0; i < MakalahPeserta.length; i++){ %>
  
            <tr>
              <td>
                <%= i + 1 %>
              </td>
              <td>
                <%= MakalahPeserta[i].pesertaName %>
              </td>
              <td>
                <%= MakalahPeserta[i].asalCabang%>
              </td>
              <td>
                <%= MakalahPeserta[i].nilaiPenulisan%>
              </td>
              <td>
                <%= MakalahPeserta[i].nilaiPenyampaian%>
              </td>
              <td>
                <%= MakalahPeserta[i].nilaiRespon%>
              </td>
              <td>
                <% if (MakalahPeserta[i]._id) { %>
                  <a href="/nilai-makalah/edit/<%= MakalahPeserta[i]._id %>" class="btn btn-sm btn-warning" title="Edit Nilai">
                    <i class="fas fa-edit"></i>
                  </a>
                <% } %>
              </td>
              <td>
                <%= MakalahPeserta[i].rataRata%>
              </td>
            </tr>
            <% } %>
        </tbody>
      </table>
    </div>
  </div>
  <!-- /.card-body -->
</div>