<table id="example2" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>No.</th>
            <th>Nama</th>
            <th>Asal Cabang</th>
            <th>Nilai Post Test</th>
            <th>Aksi</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i = 0; i < PosTest.length; i++) { %>
            <tr>
                <td><%= i + 1 %></td>
                <td><%= PosTest[i].pesertaName %></td>
                <td><%= PosTest[i].pesertaCabang%></td>
                <td><%= PosTest[i].nilaiTes%></td>
                <td style="text-align: center;">
                    <% if (PosTest[i]._id) { %>
                        <a href="/post-test/edit/<%= PosTest[i]._id %>" class="btn btn-sm btn-warning" title="Edit Nilai">
                            <i class="fas fa-edit"></i>
                        </a>
                    <% } %>
                </td>
            </tr>
        <% } %>
    </tbody>
</table>
