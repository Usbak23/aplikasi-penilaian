<div class="card-header py-3">
    <h3 class="m-0 font-weight-bold text-primary">Data Nilai Middle Peserta</h3>
</div>
<table id="example2" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>No.</th>
            <th>Nama</th>
            <th>Asal Cabang</th>
            <th>Nilai Middle Test</th>
            <th>Aksi</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i=0; i < MidTest.length; i++){ %>

            <tr>
                <td><%= i + 1 %></td>
                <td><%= MidTest[i].pesertaName %></td>
                <td><%= MidTest[i].pesertaCabang%></td>
                <td><%= MidTest[i].nilaiTes%></td>
                <td style="text-align: center;">
                    <% if (MidTest[i]._id) { %>
                        <a href="/mid-test/edit/<%= MidTest[i]._id %>" class="btn btn-sm btn-warning" title="Edit Nilai">
                            <i class="fas fa-edit"></i>
                        </a>
                    <% } %>
                </td>
            </tr>
            <% } %>
    </tbody>

</table>