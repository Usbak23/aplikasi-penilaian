<table id="example2" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>No.</th>
            <th>Nama</th>
            <th>Bobot Nilai (%)</th>
            <th>Aksi</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i=0; i < nilaiPresensi.length; i++){ %>
            <tr>
                <td><%= i + 1 %></td>
                <td><%= nilaiPresensi[i].status %></td>
                <td><%= nilaiPresensi[i].bobotNilai %></td>
                <td>
                    <form action="/nilai-presensi/delete/<%= nilaiPresensi[i]._id %>?_method=DELETE" method="post">
                        <a href="/nilai-presensi/edit/<%= nilaiPresensi[i]._id %>" class="btn btn-info btn-sm">
                            <i class="fas fa-pencil-alt"></i>
                            Ubah
                        </a>
                        <button class="btn btn-danger btn-sm" >
                            <i class="fas fa-trash"></i>
                            Hapus
                        </button>
                    </form>
                </td>
            </tr>
            <% } %>
    </tbody>

</table>