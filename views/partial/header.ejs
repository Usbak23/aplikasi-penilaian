  <style>
    .modal {
      display: none;
      position: fixed;
      top: 0;
      left: 0;
      width: 100cm;
      height: 100cm;
      background-color: rgba(0, 0, 0, 0.8);
      justify-content: center;
      align-items: center;
      opacity: 0;
      transition: opacity 0.3s ease;
    }

    .modal img {
      max-width: 100cm;
      max-height: 100cm;
    }

    .modal.active {
      display: flex;
      opacity: 1;
    }
    .disabled-link {
    pointer-events: none; /* Mencegah klik */
    opacity: 0.5; /* Membuat tampilan lebih redup */
    cursor: not-allowed; /* Menunjukkan bahwa link tidak bisa diklik */
}

